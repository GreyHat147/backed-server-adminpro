{
    "extends": "airbnb",
    "plugins": [
        "import"
    ],
    "rules": {
        "no-console": "off",
        "import/newline-after-import": "off",
        "no-unused-vars": 0,
        "max-len": 0,
        "linebreak-style": "off",
        "prefer-destructuring": "off",
        "indent": ["error", 4],
        "eol-last": "off"
    }
}